---
import Layout from '../../layouts/Layout.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';

const structuredData = {
  "@context": "https://schema.org",
  "@type": "Service",
  "name": "نقل البيانات الى اودو ERP في السعودية",
  "description": "خدمات نقل امن ودقيق لجميع بياناتك من الانظمة القديمة الى اودو مع ضمان سلامة وصحة البيانات",
  "provider": {
    "@type": "Organization",
    "name": "ToDoOps",
    "url": "https://todoops.com"
  },
  "areaServed": {
    "@type": "Country",
    "name": "Saudi Arabia"
  }
};
---

<Layout 
  title="نقل البيانات الى اودو - ToDoOps | خدمات نقل البيانات الامنة في السعودية"
  lang="ar" 
  description="خدمات نقل امن ودقيق لجميع بياناتك من الانظمة القديمة الى اودو. تحليل البيانات، تنظيف البيانات، النقل الامن، والتحقق والاختبار."
>
  <Fragment slot="head">
    <meta name="keywords" content="نقل البيانات, نقل البيانات اودو, تحويل البيانات, نقل امن, اودو السعودية, تنظيف البيانات" />
    <meta property="og:title" content="نقل البيانات الى اودو - ToDoOps" />
    <meta property="og:description" content="خدمات نقل امن ودقيق لجميع بياناتك من الانظمة القديمة الى اودو مع ضمان سلامة وصحة البيانات" />
    <link rel="canonical" href="https://todoops.com/services/data-migration" />
    <script type="application/ld+json" set:html={JSON.stringify(structuredData)}></script>
  </Fragment>

  <Header lang="ar" />
  
  <main class="py-20 bg-white min-h-screen">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <!-- Breadcrumb -->
      <nav class="mb-8" aria-label="Breadcrumb">
        <ol class="flex items-center space-x-2 rtl:space-x-reverse text-sm text-dark-600">
          <li><a href="/" class="hover:text-odoo-purple transition-colors duration-200">الرئيسية</a></li>
          <li><svg class="w-4 h-4 mx-2 rtl:rotate-180" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg></li>
          <li><a href="/services" class="hover:text-odoo-purple transition-colors duration-200">الخدمات</a></li>
          <li><svg class="w-4 h-4 mx-2 rtl:rotate-180" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"></path></svg></li>
          <li class="text-dark-900 font-medium">نقل البيانات</li>
        </ol>
      </nav>

      <!-- Hero Section -->
      <div class="text-center mb-16">
        <h1 class="text-4xl lg:text-5xl font-bold text-dark-900 mb-6">
          نقل البيانات الى اودو ERP
        </h1>
        <p class="text-xl text-odoo-purple font-medium mb-6">
          نقل امن ودقيق لجميع بياناتك الى اودو
        </p>
        <p class="text-lg text-dark-600 max-w-4xl mx-auto leading-relaxed">
          نقدم خدمات نقل امن ودقيق لجميع بياناتك من الانظمة القديمة الى اودو مع ضمان سلامة وصحة البيانات والحفاظ على تاريخ شركتك الكامل.
        </p>
      </div>

      <!-- Migration Process -->
      <div class="mb-16">
        <h2 class="text-3xl font-bold text-dark-900 text-center mb-12">
          مراحل نقل البيانات
        </h2>
        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-8">
          <div class="text-center">
            <div class="w-16 h-16 bg-odoo-accent/20 rounded-xl flex items-center justify-center mx-auto mb-4">
              <svg class="w-8 h-8 text-odoo-purple" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-dark-900 mb-3">تحليل البيانات</h3>
            <p class="text-dark-600">دراسة شاملة لبياناتك الحالية وتحديد هيكل النقل</p>
          </div>
          <div class="text-center">
            <div class="w-16 h-16 bg-green-100 rounded-xl flex items-center justify-center mx-auto mb-4">
              <svg class="w-8 h-8 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6-4h6m2 5.291A7.962 7.962 0 0112 15c-2.34 0-4.47.881-6.08 2.33l-.147.15C5.775 17.48 5.775 17.48 5.775 17.48L4.5 18.75c-.621.621-.621 1.621 0 2.242L6.742 23.234a1.5 1.5 0 002.122 0l2.242-2.242c.621-.621.621-1.621 0-2.242L9.864 17.508s0 0 0 0l.15-.147A7.962 7.962 0 0112 15c2.34 0 4.47.881 6.08 2.33l.147.15s0 0 0 0l1.242 1.242c.621.621 1.621.621 2.242 0l2.242-2.242a1.5 1.5 0 000-2.122L21.711 12.08A7.962 7.962 0 0119.5 6c0-2.34-.881-4.47-2.33-6.08l-.15-.147s0 0 0 0L15.778.531a1.5 1.5 0 00-2.122 0L11.414 2.773a1.5 1.5 0 000 2.122l1.242 1.242s0 0 0 0l.147.15A7.962 7.962 0 0115 9c0 2.34-.881 4.47-2.33 6.08l-.15.147s0 0 0 0l-1.242 1.242c-.621.621-1.621.621-2.242 0L7.794 15.227a1.5 1.5 0 00-2.122 0L3.43 17.469a1.5 1.5 0 000 2.122l2.242 2.242c.621.621 1.621.621 2.242 0l1.242-1.242s0 0 0 0l.15-.147A7.962 7.962 0 0112 21c2.34 0 4.47-.881 6.08-2.33l.147-.15s0 0 0 0l1.242-1.242c.621-.621.621-1.621 0-2.242L17.227 12.794a1.5 1.5 0 00-2.122 0L12.863 15.036s0 0 0 0l-.15.147A7.962 7.962 0 019 17.5c-2.34 0-4.47-.881-6.08-2.33l-.147-.15s0 0 0 0L1.531 13.778a1.5 1.5 0 00-2.122 0L-2.833 16.02a1.5 1.5 0 000 2.122l2.242 2.242c.621.621 1.621.621 2.242 0l1.242-1.242s0 0 0 0l.15-.147A7.962 7.962 0 006 21c0 2.34.881 4.47 2.33 6.08l.15.147s0 0 0 0l1.242 1.242c.621.621 1.621.621 2.242 0l2.242-2.242a1.5 1.5 0 000-2.122L9.964 21.863s0 0 0 0l-.147-.15A7.962 7.962 0 007.5 19c0-2.34.881-4.47 2.33-6.08l.15-.147s0 0 0 0l1.242-1.242c.621-.621.621-1.621 0-2.242L8.98 7.289a1.5 1.5 0 00-2.122 0L4.616 9.531a1.5 1.5 0 000 2.122l2.242 2.242c.621.621 1.621.621 2.242 0l1.242-1.242s0 0 0 0l.15-.147A7.962 7.962 0 0112 15z"></path>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-dark-900 mb-3">تنظيف البيانات</h3>
            <p class="text-dark-600">تنظيف وتنسيق البيانات لضمان جودتها ودقتها</p>
          </div>
          <div class="text-center">
            <div class="w-16 h-16 bg-blue-100 rounded-xl flex items-center justify-center mx-auto mb-4">
              <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7h12m0 0l-4-4m4 4l-4 4m0 6H4m0 0l4 4m-4-4l4-4"></path>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-dark-900 mb-3">النقل الامن</h3>
            <p class="text-dark-600">نقل البيانات بطريقة امنة مع النسخ الاحتياطي</p>
          </div>
          <div class="text-center">
            <div class="w-16 h-16 bg-orange-100 rounded-xl flex items-center justify-center mx-auto mb-4">
              <svg class="w-8 h-8 text-orange-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
              </svg>
            </div>
            <h3 class="text-xl font-bold text-dark-900 mb-3">التحقق والاختبار</h3>
            <p class="text-dark-600">التحقق من صحة البيانات واختبار شامل للنظام</p>
          </div>
        </div>
      </div>

      <!-- CTA Section -->
      <div class="bg-gradient-to-r from-odoo-purple to-purple-800 rounded-2xl p-8 text-white text-center">
        <h2 class="text-3xl font-bold mb-4">
          ابدا نقل بياناتك الى اودو بامان
        </h2>
        <p class="text-xl mb-6 text-purple-100">
          احصل على استشارة مجانية حول نقل بياناتك من النظام الحالي الى اودو
        </p>
        <a 
          href="/contact"
          class="inline-flex items-center bg-white text-odoo-purple px-8 py-3 rounded-lg font-semibold hover:bg-gray-50 transition-all duration-200 shadow-lg"
        >
          احجز استشارة النقل
          <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 8l4 4m0 0l-4 4m4-4H3"></path>
          </svg>
        </a>
      </div>
    </div>
  </main>

  <Footer lang="ar" />
</Layout>